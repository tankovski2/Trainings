@using FailTracker.Web.Domain
@model FailTracker.Web.Domain.IssueType
@{
    var options = Enum.GetValues(typeof(IssueType))
    .Cast<IssueType>()
    .Select(t => new SelectListItem
    {
        Text = t.ToString(),
        Value = t.ToString(),
    })
    .ToArray();

}

@Html.DropDownList("", options, new
{
	@class = "form-control",
	ng_model = Html.AngularBindingForModel()
})
